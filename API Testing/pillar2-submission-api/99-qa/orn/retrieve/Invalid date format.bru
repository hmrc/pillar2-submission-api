meta {
  name: Invalid date format
  type: http
  seq: 6
}

get {
  url: {{apiUrl}}/overseas-return-notification?accountingPeriodFrom=01-01-2024&accountingPeriodTo=31-12-2024
  body: none
  auth: none
}

params:query {
  accountingPeriodFrom: 01-01-2024
  accountingPeriodTo: 31-12-2024
}

headers {
  Authorization: {{bearer_token}}
  X-Pillar2-Id: {{testPlrId}}
  Accept: application/vnd.hmrc.1.0+json
}

tests {
  test("should return 400 Bad Request", function() {
    expect(res.getStatus()).to.equal(400);
  });
  
  test("should contain correct error message", function() {
    expect(res.body.code).to.equal("INVALID_DATE_FORMAT");
    expect(res.body.message).to.equal("Invalid date format");
  });
}

