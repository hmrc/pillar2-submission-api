meta {
  name: X-Pillar2-Id mismatch
  type: http
  seq: 8
}

put {
  url: {{apiUrl}}/overseas-return-notification
  body: json
  auth: none
}

headers {
  Authorization: {{bearer_token}}
  X-Pillar2-Id: XEPLR9999999999
  Accept: application/vnd.hmrc.1.0+json
}

body:json {
  {
    "accountingPeriodFrom": "2024-01-01",
    "accountingPeriodTo": "2024-12-31",
    "filedDateGIR": "2025-01-10",
    "countryGIR": "US",
    "reportingEntityName": "Test Entity",
    "TIN": "US12345678",
    "issuingCountryTIN": "US"
  }
}

tests {
  test("should return 400 Bad Request", function() {
    expect(res.getStatus()).to.equal(400);
  });
  
  test("should contain correct error message", function() {
    expect(res.body.code).to.equal("INCORRECT_HEADER_VALUE");
  });
}

