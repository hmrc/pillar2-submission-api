meta {
  name: Empty liable entities
  type: http
  seq: 2
}

put {
  url: {{pillar2Url}}/amend-uk-tax-return
  body: json
  auth: none
}

headers {
  X-Pillar2-Id: {{testPlrId}}
  Authorization: {{bearerToken}}
}

body:json {
  {
    "accountingPeriodFrom": "2024-01-01",
    "accountingPeriodTo": "2024-12-31",
    "obligationMTT": false,
    "electionUKGAAP": true,
    "liabilities": {
      "electionDTTSingleMember": false,
      "electionUTPRSingleMember": false,
      "numberSubGroupDTT": 1,
      "numberSubGroupUTPR": 1,
      "totalLiability": 10000.99,
      "totalLiabilityDTT": 5000.99,
      "totalLiabilityIIR": 4000,
      "totalLiabilityUTPR": 10000.99,
      "liableEntities": [
        
      ]
    }
  }
}

tests {
  test("Response contains correct error code", function() {
    expect(res.body.code).to.equal("002");
  });
  
  test("Response contains correct error message", function() {
    expect(res.body.message).to.equal("Liable entities must not be empty");
  });
  
  test("Status code is 422", function() {
    expect(res.status).to.equal(422);
  });
  
  
}
